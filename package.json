{
  "name": "n8n-nodes-kan",
  "version": "1.0.0",
  "description": "Custom node for Kanboard integration inside n8n workflows",
  "files": [
    "dist"
  ],
  "keywords": [
    "n8n",
    "n8n-nodes", 
    "kanboard",
    "kan",
    "project management",
    "task management"
  ],
  "author": "Stas Dashchinksiy",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts",
    "package": "npm run build && npm pack"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/KanApi.credentials.js"
    ],
        "nodes": [
          "dist/nodes/Kan/KanTask.node.js",
          "dist/nodes/Kan/KanProject.node.js",
          "dist/nodes/Kan/KanBoard.node.js",
          "dist/nodes/Kan/KanComment.node.js",
          "dist/nodes/Kan/KanWebhook.node.js",
          "dist/nodes/Kan/KanUser.node.js"
        ]
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seu-usuario/n8n-nodes-kan.git"
  },
  "publishConfig": {
    "access": "public"
  }
}